#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num, *arr, i,sum =0;
    scanf("%d", &num);
    arr = (int*) malloc(num * sizeof(int));
    for(i = 0; i < num; i++) {
        scanf("%d", arr + i);
    }


    for(i = 0; i < num; i++){
        //printf("%d ", *(arr + i));
        sum = sum + *(arr+i);
        
    }
    printf("%d ",sum);
    return 0;
}
